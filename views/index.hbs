<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1" />
    <script src="/js/browser-deeplink.js" type="text/javascript"></script>
    <script type="text/javascript">
      deeplink.setup({
        iOS: {
          appId: '1548625164',
          appName: 'c2d',
        },
        android: {
          appId: 'com.facebook.katana',
        },
      });

      setTimeout(function () {
        deeplink.open('fb://profile');
        return false;
      }, 2000);
    </script>
    <style>
      *,
      *:before,
      *:after {
        box-sizing: border-box;
      }
      a {
        font-family: monospace;
        display: block;
        width: 100%;
        margin: 10px 0;
        padding: 25px;
        background: #060;
        color: #fff;
        text-align: center;
        font-size: 20px;
      }
    </style>
  </head>
  <body></body>
</html>
